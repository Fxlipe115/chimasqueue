    "slack-payload": "^1.0.11"
  },
  "devDependencies": {
    "@types/chai": "^4.2.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "chai": "^4.2.0",
    "firebase-functions-test": "^0.1.6",
    "mocha": "^6.2.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.12.0",
    "typescript": "^3.6.3"
  },
  "private": true
